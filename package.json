{
  "name": "rangular-app",
  "description": "Ranis components built on Agular Material",
  "version": "1.0.0",
  "keywords": [
    "angular",
    "ranis",
    "material",
    "components"
  ],
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:prod": "ng serve --aot --prod --source-map=false --optimization",
    "copy-license-mat": "cp ./LICENSE ./dist/rangular-material",
    "copy-readme-mat": "cp ./README.md ./dist/rangular-material",
    "copy-files-mat": "npm run copy-license-mat && npm run copy-readme-mat",
    "build-lib-mat": "ng build --prod rangular-material",
    "npm-pack-mat": "cd dist/rangular-material && npm pack",
    "package-mat": "npm run build-lib-mat && npm run copy-files-mat && npm run npm-pack-mat",
    "copy-readme-common": "cp ./README.md ./dist/rangular-common",
    "copy-license-common": "cp ./LICENSE ./dist/rangular-common",
    "copy-files-common": "npm run copy-license-common && npm run copy-readme-common",
    "build-lib-common": "ng build --prod rangular-common",
    "npm-pack-common": "cd dist/rangular-common && npm pack",
    "package-common": "npm run build-lib-common && npm run copy-files-common && npm run npm-pack-common",
    "package": "npm run package-common && npm run package-mat",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@covalent/core": "^2.0.0-beta.4",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "rangular-common": "file:dist/rangular-common/rangular-common-0.0.6.tgz",
    "rangular-material": "file:dist/rangular-material/rangular-material-0.0.5.tgz",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.117",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6"
  }
}
