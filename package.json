{
  "name": "rangular-app",
  "description": "Ranis components built on Angular Material",
  "version": "13.0.0",
  "keywords": [
    "angular",
    "ranis",
    "material",
    "crud",
    "components"
  ],
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:prod": "ng serve --aot --configuration production --source-map=false --optimization",
    "copy-license-mat": "cp ./LICENSE ./dist/rangular-material",
    "copy-readme-mat": "cp ./README.md ./dist/rangular-material",
    "copy-files-mat": "npm run copy-license-mat && npm run copy-readme-mat",
    "build-lib-mat": "ng build --configuration production rangular-material",
    "npm-pack-mat": "cd dist/rangular-material && npm pack",
    "package-mat": "npm run build-lib-mat && npm run copy-files-mat && npm run npm-pack-mat",
    "copy-readme-common": "cp ./README.md ./dist/rangular-common",
    "copy-license-common": "cp ./LICENSE ./dist/rangular-common",
    "copy-files-common": "npm run copy-license-common && npm run copy-readme-common",
    "build-lib-common": "ng build --configuration production rangular-common",
    "npm-pack-common": "cd dist/rangular-common && npm pack",
    "package-common": "npm run build-lib-common && npm run copy-files-common && npm run npm-pack-common",
    "copy-readme-crud": "cp ./README.md ./dist/rangular-crud",
    "copy-license-crud": "cp ./LICENSE ./dist/rangular-crud",
    "copy-files-crud": "npm run copy-license-crud && npm run copy-readme-crud",
    "build-lib-crud": "ng build --configuration production rangular-crud",
    "npm-pack-crud": "cd dist/rangular-crud && npm pack",
    "package-crud": "npm run build-lib-crud && npm run copy-files-crud && npm run npm-pack-crud",
    "copy-readme-pipes": "cp ./README.md ./dist/rangular-pipes",
    "copy-license-pipes": "cp ./LICENSE ./dist/rangular-pipes",
    "copy-files-pipes": "npm run copy-license-pipes && npm run copy-readme-pipes",
    "build-lib-pipes": "ng build --configuration production rangular-pipes",
    "npm-pack-pipes": "cd dist/rangular-pipes && npm pack",
    "package-pipes": "npm run build-lib-pipes && npm run copy-files-pipes && npm run npm-pack-pipes",
    "copy-readme-ngxs": "cp ./README.md ./dist/rangular-ngxs",
    "copy-license-ngxs": "cp ./LICENSE ./dist/rangular-ngxs",
    "copy-files-ngxs": "npm run copy-license-ngxs && npm run copy-readme-ngxs",
    "build-lib-ngxs": "ng build --configuration production rangular-ngxs",
    "npm-pack-ngxs": "cd dist/rangular-ngxs && npm pack",
    "package-ngxs": "npm run build-lib-ngxs && npm run copy-files-ngxs && npm run npm-pack-ngxs",
    "package": "npm run package-common && npm run package-mat && npm run package-crud && npm run package-pipes && npm run package-ngxs",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --source ./node_modules --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@covalent/core": "^4.0.0",
    "@ngneat/transloco": "^3.2.0",
    "@ngxs/router-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "ag-grid-angular": "^27.1.1",
    "ag-grid-community": "^27.1.0",
    "lodash": "^4.17.21",
    "moment-jalaali": "^0.9.4",
    "rangular-common": "file:dist/rangular-common/rangular-common-13.0.2.tgz",
    "rangular-ngxs": "file:dist/rangular-ngxs/rangular-ngxs-13.0.0.tgz",
    "rangular-pipes": "file:dist/rangular-pipes/rangular-pipes-13.0.0.tgz",
    "rangular-crud": "file:dist/rangular-crud/rangular-crud-13.0.0.tgz",
    "rangular-material": "file:dist/rangular-material/rangular-material-13.0.1.tgz",
    "rxjs": "~6.6.3",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-devkit/core": "^13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@ngneat/spectator": "^10.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.117",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.0",
    "node-sass": "^4.14.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.2"
  }
}
