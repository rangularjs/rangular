{
  "name": "rangular-app",
  "description": "Ranis components built on Angular Material",
  "version": "8.0.0",
  "keywords": [
    "angular",
    "ranis",
    "material",
    "crud",
    "components"
  ],
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:prod": "ng serve --aot --prod --source-map=false --optimization",
    "copy-license-mat": "cp ./LICENSE ./dist/rangular-material",
    "copy-readme-mat": "cp ./README.md ./dist/rangular-material",
    "copy-files-mat": "npm run copy-license-mat && npm run copy-readme-mat",
    "build-lib-mat": "ng build --prod rangular-material",
    "npm-pack-mat": "cd dist/rangular-material && npm pack",
    "package-mat": "npm run build-lib-mat && npm run copy-files-mat && npm run npm-pack-mat",
    "copy-readme-common": "cp ./README.md ./dist/rangular-common",
    "copy-license-common": "cp ./LICENSE ./dist/rangular-common",
    "copy-files-common": "npm run copy-license-common && npm run copy-readme-common",
    "build-lib-common": "ng build --prod rangular-common",
    "npm-pack-common": "cd dist/rangular-common && npm pack",
    "package-common": "npm run build-lib-common && npm run copy-files-common && npm run npm-pack-common",
    "copy-readme-crud": "cp ./README.md ./dist/rangular-crud",
    "copy-license-crud": "cp ./LICENSE ./dist/rangular-crud",
    "copy-files-crud": "npm run copy-license-crud && npm run copy-readme-crud",
    "build-lib-crud": "ng build --prod rangular-crud",
    "npm-pack-crud": "cd dist/rangular-crud && npm pack",
    "package-crud": "npm run build-lib-crud && npm run copy-files-crud && npm run npm-pack-crud",
    "copy-readme-pipes": "cp ./README.md ./dist/rangular-pipes",
    "copy-license-pipes": "cp ./LICENSE ./dist/rangular-pipes",
    "copy-files-pipes": "npm run copy-license-pipes && npm run copy-readme-pipes",
    "build-lib-pipes": "ng build --prod rangular-pipes",
    "npm-pack-pipes": "cd dist/rangular-pipes && npm pack",
    "package-pipes": "npm run build-lib-pipes && npm run copy-files-pipes && npm run npm-pack-pipes",
    "package": "npm run package-common && npm run package-mat && npm run package-crud && npm run package-pipes",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@covalent/core": "^2.1.1",
    "ag-grid-angular": "^22.0.0",
    "ag-grid-community": "^22.0.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "moment-jalaali": "^0.9.1",
    "rangular-common": "file:dist/rangular-common/rangular-common-8.0.0.tgz",
    "rangular-crud": "file:dist/rangular-crud/rangular-crud-8.1.0.tgz",
    "rangular-material": "file:dist/rangular-material/rangular-material-8.0.0.tgz",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "@types/lodash": "^4.14.117",
    "tslib": "^1.9.0"
  }
}
